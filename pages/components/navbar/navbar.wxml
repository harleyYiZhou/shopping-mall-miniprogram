<!--pages/components/navbar/navbar.wxml-->
 <template name="page-nav"> 
  <view class="type-container">
    <scroll-view class="type-navbar" scroll-x="true">
      <view class="type-box" wx:for="{{pages}}" wx:for-item="item" wx:key="id">
        <view id="{{index}}" class="type-navbar-item {{activeCategoryId == index ? 'type-item-on' : ''}}" bindtap="tabClick" data-items="{{}}">
          {{item.pageTitle}}
        </view>
      </view>
    </scroll-view>
  </view>
 </template> 