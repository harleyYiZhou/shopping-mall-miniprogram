/* pages/memberships/memberships.less */
.coupons {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 15rpx;
}
.coupons .icon {
  margin-top: 20rpx;
  width: 120rpx;
  height: 120rpx;
}
.coupons .title {
  font-size: 60rpx;
}
.coupons .list {
  width: 100%;
  padding-left: 30rpx;
  padding-bottom: 20rpx;
  margin: 10rpx 0;
  box-sizing: border-box;
  border-radius: 15rpx;
  background-color: #fff;
}
.coupons .coupon {
  padding-left: 10rpx;
  width: 100%;
  box-sizing: border-box;
}
.coupons .border-bottom {
  border-bottom: 1rpx solid #e0e0e0;
}
.coupons .until {
  color: #9e9e9e;
  font-size: 80%;
}
